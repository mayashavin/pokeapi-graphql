{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server",
      "env": {
        "ENGINE_API_KEY": "@engine_api_key"
      }
    }
  ],
  "routes": [
    {
      "src": "./*",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "ENGINE_API_KEY": "@engine_api_key"
  }
}
